<root>    
	<LoginWeb des="管理平台登录页面">
		<LoginAccountInput des = "用户名">
			<by>id</by>
			<value>login_text_username-input</value>					
		</LoginAccountInput>
		<LoginPasswordInput des = "密码">
			<by>id</by>
			<value>login_text_password-input</value>
		</LoginPasswordInput>
		<LoginCheckCode des = "验证码">
			<by>id</by>
			<value>login_text_checkcode-input</value>
		</LoginCheckCode>
		<LoginStartButton des = "登录按钮">
			<by>xpath</by>
			<value>//button</value>
			<index>0</index>
		</LoginStartButton>
		<LoginErrorTipText des = "错误提示">
			<by>id</by>
			<value>x-auto-17-content</value>
		</LoginErrorTipText>
		<LoginErrorTipBtn des = "错误提示-确定">
			<by>css selector</by>
			<value>button</value>
			<index>2</index>
		</LoginErrorTipBtn>
	</LoginWeb>	
</root>